      DTS:ThreadHint="1">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{67D2A42E-6B09-49C6-8380-CD91760F0A1B}"
          SQLTask:SqlStatementSource="IF NOT EXISTS (&#xA;    SELECT * FROM sys.tables t&#xA;    JOIN sys.schemas s ON t.schema_id = s.schema_id&#xA;    WHERE t.name = 'dim_customers' AND s.name = 'gold'&#xA;)&#xA;BEGIN&#xA;    CREATE TABLE gold.dim_customers (&#xA;        customer_key INT IDENTITY(1,1) PRIMARY KEY,       &#xA;        customer_id NVARCHAR(50),&#xA;        customer_crm_id INT,&#xA;        firstname NVARCHAR(50),&#xA;        lastname NVARCHAR(50),&#xA;        customer_country NVARCHAR(50),&#xA;        marital_status NVARCHAR(50),&#xA;        gender NVARCHAR(50),&#xA;        birthday DATE,&#xA;        join_crm_date DATE&#xA;    );&#xA;END;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Create Gold DimDates"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{e48b2d9c-a1d0-48bc-b981-dfb25de9e01c}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="Create Gold DimDates"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2022; © 2022 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{67D2A42E-6B09-49C6-8380-CD91760F0A1B}"
          SQLTask:SqlStatementSource="IF NOT EXISTS (&#xA;    SELECT * &#xA;    FROM sys.tables t&#xA;    JOIN sys.schemas s ON t.schema_id = s.schema_id&#xA;    WHERE t.name = 'dim_dates' AND s.name = 'gold'&#xA;)&#xA;BEGIN&#xA;    -- Tạo bảng dim_dates nếu chưa tồn tại&#xA;    CREATE TABLE gold.dim_dates (&#xA;        date_id        DATE PRIMARY KEY,&#xA;        day            INT,&#xA;        month          INT,&#xA;        year           INT,&#xA;        quarter        INT,&#xA;        day_of_week    INT,         -- 1 = Sunday, 7 = Saturday (theo cấu hình mặc định)&#xA;        day_name       NVARCHAR(10),&#xA;        month_name     NVARCHAR(10),&#xA;        is_weekend     BIT&#xA;    );&#xA;&#xA;    -- Khai báo biến ngày bắt đầu và ngày kết thúc&#xA;    DECLARE @start_date DATE;&#xA;    DECLARE @end_date DATE;&#xA;&#xA;    -- Xác định phạm vi ngày cần tạo&#xA;    SELECT &#xA;        @start_date = MIN(order_date), &#xA;        @end_date = DATEADD(YEAR, 1, MAX(due_date))&#xA;    FROM silver.crm_sales_details;&#xA;&#xA;    -- Duyệt qua từng ngày và chèn vào bảng dim_dates&#xA;    WHILE @start_date &lt;= @end_date&#xA;    BEGIN&#xA;        INSERT INTO gold.dim_dates (&#xA;            date_id,&#xA;            day,&#xA;            month,&#xA;            year,&#xA;            quarter,&#xA;            day_of_week,&#xA;            day_name,&#xA;            month_name,&#xA;            is_weekend&#xA;        )&#xA;        SELECT&#xA;            @start_date,&#xA;            DAY(@start_date),&#xA;            MONTH(@start_date),&#xA;            YEAR(@start_date),&#xA;            DATEPART(QUARTER, @start_date),&#xA;            DATEPART(WEEKDAY, @start_date),&#xA;            DATENAME(WEEKDAY, @start_date),&#xA;            DATENAME(MONTH, @start_date),&#xA;            CASE &#xA;                WHEN DATEPART(WEEKDAY, @start_date) IN (1, 7) THEN 1 &#xA;                ELSE 0 &#xA;            END;&#xA;&#xA;        -- Tăng ngày lên 1&#xA;        SET @start_date = DATEADD(DAY, 1, @start_date);&#xA;    END&#xA;END;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
