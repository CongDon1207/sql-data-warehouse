          SQLTask:SqlStatementSource="&#xA;IF NOT EXISTS (&#xA;    SELECT * FROM sys.schemas WHERE name = 'gold'&#xA;)&#xA;BEGIN&#xA;    EXEC('CREATE SCHEMA gold');&#xA;END;&#xA;&#xA;IF NOT EXISTS (&#xA;    SELECT * &#xA;    FROM sys.tables t&#xA;    INNER JOIN sys.schemas s ON t.schema_id = s.schema_id&#xA;    WHERE t.name = 'dim_products' AND s.name = 'gold'&#xA;)&#xA;BEGIN&#xA;    CREATE TABLE [gold].[dim_products] (&#xA;        product_key INT IDENTITY(1,1) PRIMARY KEY,&#xA;        product_crm_id INT,&#xA;        product_id NVARCHAR(50),&#xA;        product_name NVARCHAR(50),&#xA;        category_id NVARCHAR(50),&#xA;        category_name NVARCHAR(50),&#xA;        subcategory_name NVARCHAR(50),&#xA;        maintenance_flag BIT,&#xA;        product_cost INT,&#xA;        product_line NVARCHAR(50),&#xA;        start_date DATE,&#xA;        end_date DATE&#xA;    );&#xA;END;&#xA;GO" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
